	keywords = {Canine, Neoplasia, Extra-skeletal osteosarcoma, Left atrium, DOGS, CARDIAC TUMORS},
	pages = {209--215},
	annote = {Times Cited in Web of Science Core Collection:  0Total Times Cited:  0Cited Reference Count:  19},
