	keywords = {IDENTIFICATION, MODEL, RETINITIS-PIGMENTOSA, SEQUENCE, CONE-ROD DYSTROPHY, LEBER CONGENITAL AMAUROSIS, ATROPHY, DACHSHUNDS, GENE-MUTATIONS, PROTEINS},
