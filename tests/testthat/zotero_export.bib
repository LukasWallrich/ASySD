	keywords = {syringomyelia, Chiari-like malformation, dogs, Cerebrospinal fluid, Chiari, PATHOGENESIS, SIZE, BLOOD-CELL COUNT, CONTAMINATION, LUMBAR, MYELIN BASIC-PROTEIN, RIDDLE, SAMPLES},
