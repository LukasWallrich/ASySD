@article{bauer_relative_2010,
	title = {Relative quantification of white blood cell mitochondrial {DNA} and assessment of mitochondria by use of transmission electron microscopy in {English} {Springer} {Spaniels} with and without retinal dysplasia},
	volume = {71},
