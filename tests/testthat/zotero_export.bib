	month = nov,
	year = {1993},
	keywords = {GENE, DOGS, EXPRESSION, DOG, CANCER, C-MYC ONCOPROTEIN, CLINICAL-SIGNIFICANCE, DNA PLOIDY, FLOW CYTOMETRY, LYMPHOMAS, MYELOMA, ONCOGENE PRODUCT, PLASMACYTOMA, QUANTITATION},
	pages = {505--511},
