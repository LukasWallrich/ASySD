	keywords = {canine, PREVALENCE, RESPONSES, STRESS, DOMESTICATION, HERITABILITY, development, ehavior, fear, FEARFULNESS, GERMAN-SHEPHERD DOGS, PERIOD, SALIVA CORTISOL, salivary cortisol, STIMULI},
