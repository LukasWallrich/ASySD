	number = {12},
	journal = {KLEINTIERPRAXIS},
	author = {Haggstrom, J and Boswood, A and O'Grady, M and Jons, O and Smith, S and Swift, S and Borgarelli, M and Gavaghan, B and Kresken, JG and Patteson, M and Ablad, B and Bussadori, CM and Giaus, T and Kovacevic, A and Rapp, M and Santilli, RA and Tidholm, A and Eriksson, A and Belanger, MC and Deinert, M and Little, CJL and Kvart, C and French, A and Ronn-Landbo, M and Wess, G and Eggertsdottir, AV and O'Sullivan, ML and Schneider, M and Lombard, CW and Dukes-McEwan, J and Willis, R and Louvet, A and DiFruscia, R},
