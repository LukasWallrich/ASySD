	keywords = {Canine, Centronuclear myopathy, Myotubularin myopathy, IDENTIFICATION, DOMAIN, FAMILY, FIBERS, FRAMEWORK, PHENOTYPE, PHOSPHATASE MYOTUBULARIN, PROTEIN, SEQUENCE},
