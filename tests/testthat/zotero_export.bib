	keywords = {dog, Canis lupus familiaris, fatty acid disorder, metabolism, neurology, precision medicine, seizure, IDENTIFICATION, MUTATION, METABOLISM, FATTY-ACIDS, GENOME, LEADS, SEIZURES},
