	keywords = {DISEASE, DERMATITIS, INFECTION, INFLAMMATION, GRANULOMATOUS LYMPHADENITIS, LYMPHADENOPATHY, RESEMBLING JUVENILE CELLULITIS, STERILE NODULAR PANNICULITIS},
