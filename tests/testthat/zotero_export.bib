	keywords = {Syringomyelia, Chiari malformation, CKCS dog breed, Cranial MRI measurements, Whole genome association study, DOGS, EXPRESSION, CRANIOCERVICAL JUNCTION, CAUDAL CRANIAL FOSSA, SIGNS, PROTOCADHERINS},
