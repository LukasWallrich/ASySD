	year = {2002},
	keywords = {dog, platelets, macrothrombocytopenia, GIANT PLATELET DISORDERS, MACROTHROMBOCYTOPENIA, MITRAL-VALVE PROLAPSE, inherited condition},
	pages = {169--173},
	annote = {Times Cited in Web of Science Core Collection:  39Total Times Cited:  40Cited Reference Count:  21},
