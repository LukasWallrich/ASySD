	number = {1},
	journal = {VETERINARY PATHOLOGY},
	author = {KUHL, KA and SHOFER, FS and GOLDSCHMIDT, MH},
	month = jan,
	year = {1994},
	keywords = {IGG, ACANTHOLYSIS, CADHERIN FAMILY, CELL-ADHESION MOLECULES, DESMOGLEIN, DOMESTIC-ANIMALS, PEMPHIGUS FOLIACEUS, PLASMINOGEN-ACTIVATOR, SUPERFICIAL FOLLICULITIS},
