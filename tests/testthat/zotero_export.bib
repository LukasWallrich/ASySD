	year = {2020},
	keywords = {Brody disease, Brody syndrome, canine, congenital pseudomyotonia, dog, exercise-induced muscle stiffness, hypertonia, involuntary muscle contraction, myotonia, paramyotonia congenita, SCN4A, GENE, DIAGNOSIS, MUSCLE-FIBERS, RETICULUM CA2+ ATPASE, SERCA1},
	pages = {253--257},
