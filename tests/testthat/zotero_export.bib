	keywords = {Malware Analysis, Malware Characterization, Taint Analysis, Virtual Machine Introspection},
	pages = {471--478},
	annote = {Times Cited in Web of Science Core Collection:  0Total Times Cited:  0Cited Reference Count:  28},
}

