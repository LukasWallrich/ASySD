@article{podberscek_environmental_1997,
	title = {Environmental influences on the expression of aggressive behaviour in {English} {Cocker} {Spaniels}},
	volume = {52},
	issn = {0168-1591},
	doi = {10.1016/S0168-1591(96)01124-0},
	abstract = {A group of 'low' (n=217) and 'high' (n=218) aggression purebred English Cocker Spaniels were compared in relation to demographics and owner interactions. Owners of 'low' aggression dogs were more likely to be: older (65 years +; chi(2)=18.753, P{\textless}0.01) and more attached to their dogs (U=20346, P{\textless}0.001). Dogs in the 'high' aggression group were: significantly more likely to be of a solid colour (chi(2)=38.13, P{\textless}0.001); more likely to have been chosen for pet purposes only (chi(2)=25.161, P{\textless}0.001); more likely to have suffered an illness during the first 16 weeks of life (chi(2)=14.899, P{\textless}0.001); groomed less often (t=2.252, P{\textless}0.05); given less time for walks/exercise (t=2.618, P{\textless}0.01); slow in obeying commands (U=17967.5, P{\textless}0.001), more likely to pull on the lead (U=16663, P{\textless}0.001); and more likely to react to loud or high-pitched noises (chi(2)=14.142, P{\textless}0.001). Factors often quoted to be important in the development of dominance-related aggression, such as feeding the dog before the owner eats, a lack of obedience training, and playing competitive games with the dog, were not found to be significantly different between the two groups. Determining the importance of various factors in the development of canine aggression will enable us to better advise owners in the rearing of their dogs. (C) 1997 Elsevier Science B.V.},
